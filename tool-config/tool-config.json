{
  "config": {
  },
  "ltiKey": {
    "tool_configuration": {
      "settings": {
        "public_jwk": {},
        "title": "Visit Sub Account$TITLE_SUFFIX",
        "description": "Presents a link to the parent subaccount.",
        "target_link_uri": "$TOOL_URL",
        "oidc_initiation_url": "$TOOLS_SERVER_URL/lti/login_initiation/",
        "public_jwk_url": "$TOOLS_SERVER_URL/.well-known/jwks.json",
        "scopes": [],
        "extensions": [
          {
            "platform": "canvas.instructure.com",
            "settings": {
            "placements": [
                {
                  "placement": "course_settings_sub_navigation",
                  "message_type": "LtiResourceLinkRequest",
                  "canvas_icon_class": "icon-settings"
                }
              ]
            },
            "privacy_level": "public"
          }
        ],
        "custom_fields": {    
          "canvas_account_id": "$Canvas.account.id",
          "canvas_base_url": "$CANVAS_PROVIDER_URL"
        }
      }
    },
    "developer_key": {
      "name": "Visit Sub Account$TITLE_SUFFIX",
      "redirect_uris": "$TOOLS_SERVER_URL/lti/login",
      "scopes": []
    }
  },
  
  "apiKey"  : null,

  "toolReg": {
    "lti": {
      "registrationId": "$LTI_REGISTRATION_ID",
      "clientName": "Visit Sub Account$TITLE_SUFFIX",
      "clientId": "$LTI_DEV_ID",
      "clientSecret": "$LTI_DEV_KEY",
      "clientAuthenticationMethod": "client_secret_basic",
      "authorizationGrantType": "implicit",
      "redirectUri": "{baseUrl}/lti/login",
      "scopes": [
        "openid"
      ],
      "providerDetails": {
        "authorizationUri": "$CANVAS_PROVIDER_URL/api/lti/authorize_redirect",
        "tokenUri": "$CANVAS_PROVIDER_URL/login/oauth2/token",
        "userInfoEndpoint": null,
        "jwkSetUri": "$CANVAS_PROVIDER_URL/api/lti/security/jwks",
        "issuerUri": null,
        "configurationMetadata": {}
      }
    },
    "proxy" :  null, 
    "origins": [
      "$TOOL_URL"
    ],
    "secret": null,
    "issuer": "$TOOLS_SERVER_URL",
    "nrpsAllowedRoles": []
  }
}
